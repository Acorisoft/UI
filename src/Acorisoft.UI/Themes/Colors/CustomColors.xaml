<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml">
    <Color x:Key="UI.Color.Favorite.Purple">#6E00FF</Color>
    <Color x:Key="UI.Color.Favorite.Yellow">#FFAB01</Color>
    <Color x:Key="UI.Color.Callout.Color1">#1b809e</Color>
    <Color x:Key="UI.Color.Callout.Color2">#ce4844</Color>
    <Color x:Key="UI.Color.Callout.Color3">#aa6708</Color> 
    <SolidColorBrush x:Key="UI.Brushes.Favorite.Purple" Color="{StaticResource UI.Color.Favorite.Purple}"/>
    <SolidColorBrush x:Key="UI.Brushes.Favorite.Yellow" Color="{StaticResource UI.Color.Favorite.Yellow}"/>
    <SolidColorBrush x:Key="UI.Brushes.Callout.Color1" Color="{StaticResource UI.Color.Callout.Color1}"/>
    <SolidColorBrush x:Key="UI.Brushes.Callout.Color2" Color="{StaticResource UI.Color.Callout.Color2}"/>
    <SolidColorBrush x:Key="UI.Brushes.Callout.Color3" Color="{StaticResource UI.Color.Callout.Color3}"/>
</ResourceDictionary>